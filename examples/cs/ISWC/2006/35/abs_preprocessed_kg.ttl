local:s5	a	rdf:Statement	;
	rdfs:label	"alternative ontology languages based entirely on logic programming"	.

local:s4	a	rdf:Statement	;
	rdfs:label	"resulting in proposals for alternative ontology languages"	.

local:s3	a	rdf:Statement	;
	rdfs:label	"this sparked a heated debate in the the semantic web community"	.

local:s2	a	rdf:Statement	;
