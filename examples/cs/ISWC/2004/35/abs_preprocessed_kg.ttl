#### Statements ####

local:s8.2	a	rdf:Statement	;
	rdfs:label	"users explore the ontology"	.

local:s8.1	a	rdf:Statement	;
	rdfs:label	"most ontologies help users explore the ontology"	.

local:s8	a	rdf:Statement	;
	rdfs:label	"the navigational support offered by the semantic relations in most ontologies to help users explore the ontology"	.

local:s7	a	rdf:Statement	;
	rdfs:label	"within the same setting we study retrieval methods"	.

local:s6	a	rdf:Statement	;
	rdfs:label	"retrieval methods that complement the navigational support"	.

local:s5	a	rdf:Statement	;
	rdfs:label	"we evaluate we methods within the setting of digital library project"	.

local:s4	a	rdf:Statement	;
	rdfs:label	"methods within the setting of digital library project using information retrieval evaluation methodology"	.

local:s3.1	a	rdf:Statement	;
	rdfs:label	"we suggest new concepts"	.

local:s3	a	rdf:Statement	;
	rdfs:label	"we use collocationbased keyword extraction to suggest new concepts"	.

local:s2.2	a	rdf:Statement	;
	rdfs:label	"automate the population of ontologies with instances"	.

local:s2.1	a	rdf:Statement	;
	rdfs:label	"we study the generation of hyperlinks to automate the population of ontologies with instances"	.

local:s2	a	rdf:Statement	;
	rdfs:label	"we use collocationbased keyword extraction to study the generation of hyperlinks to automate the population of ontologies with instances"	.

local:s1	a	rdf:Statement	;
	rdfs:label	"information retrieval can contribute towards the effective usage of ontologies"	.

local:s0	a	rdf:Statement	;
	rdfs:label	"information retrieval can contribute towards the construction of ontologies"	.


